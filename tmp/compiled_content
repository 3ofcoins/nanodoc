  <span class="k">font</span><span class="o">:</span> <span class="m">0</span><span class="o">/</span><span class="m">0</span> a;
  <span class="k">color</span><span class="o">:</span> <span class="k">transparent</span>;
  <span class="k">text-shadow</span><span class="o">:</span> <span class="k">none</span>;
  <span class="k">background-color</span><span class="o">:</span> <span class="k">transparent</span>;
  <span class="k">border</span><span class="o">:</span> <span class="m">0</span>;
}

