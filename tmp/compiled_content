     <span class="o">-</span>moz<span class="o">-</span>box<span class="o">-</span>shadow<span class="o">:</span> <span class="k">inset</span> <span class="m">0</span> <span class="m">1px</span> <span class="m">0</span> rgba(<span class="m">255</span><span class="o">,</span> <span class="m">255</span><span class="o">,</span> <span class="m">255</span><span class="o">,</span> <span class="m">0</span><span class="o">.</span><span class="m">1</span>)<span class="o">,</span> <span class="m">0</span> <span class="m">1px</span> <span class="m">0</span> rgba(<span class="m">255</span><span class="o">,</span> <span class="m">255</span><span class="o">,</span> <span class="m">255</span><span class="o">,</span> <span class="m">0</span><span class="o">.</span><span class="m">075</span>);
