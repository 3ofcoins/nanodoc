  <span class="k">line-height</span><span class="o">:</span> <span class="m">30px</span>;
}

<span class="nc">.pull-right</span> {
  <span class="k">float</span><span class="o">:</span> <span class="k">right</span>;
}

<span class="nc">.pull-left</span> {
  <span class="k">float</span><span class="o">:</span> <span class="k">left</span>;
}

