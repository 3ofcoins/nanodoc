  fill_missing_directories
  <span class="vi">@items</span><span class="o">[</span><span class="s1">'/'</span><span class="o">]</span><span class="o">[</span><span class="ss">:label</span><span class="o">]</span> <span class="o">=</span> <span class="no">File</span><span class="o">.</span>basename(<span class="vi">@config</span><span class="o">[</span><span class="ss">:source_dir</span><span class="o">]</span>)
