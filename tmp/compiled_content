  <span class="k">margin-right</span><span class="o">:</span> <span class="m">2px</span>;
}

<span class="nc">.nav-list</span> <span class="nc">.divider</span> {
  <span class="o">*</span><span class="k">width</span><span class="o">:</span> <span class="m">100</span><span class="o">%</span>;
