     <span class="o">-</span>moz<span class="o">-</span>box<span class="o">-</span>sizing<span class="o">:</span> <span class="k">border</span><span class="o">-</span>box;
          box<span class="o">-</span>sizing<span class="o">:</span> <span class="k">border</span><span class="o">-</span>box;
