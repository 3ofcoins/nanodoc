}

<span class="nc">.clearfix</span><span class="nd">:before</span><span class="o">,</span>
<span class="nc">.clearfix</span><span class="nd">:after</span> {
  <span class="k">display</span><span class="o">:</span> table;
  <span class="k">line-height</span><span class="o">:</span> <span class="m">0</span>;
  <span class="k">content</span><span class="o">:</span> <span class="s2">""</span>;
}

